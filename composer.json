{
	"name": "tas2580/opensearch",
	"type": "phpbb-extension",
	"description": "Add opensearch support to the forum.",
	"homepage": "https://tas2580.net/downloads/phpbb-opensearch/",
	"version": "0.1.0",
	"keywords": ["phpbb", "extension", "search"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "tas2580",
			"homepage": "https://tas2580.net",
			"email": "mail@tas2580.net",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Opensearch",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.4-RC1,<3.2.0@dev"
		},
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/tas2580/userinfo/master",
			"filename": "version-check.json",
			"ssl": true
		}
	}
}